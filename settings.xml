      </activeProfiles>
      <profiles>
        <profile>
          <id>nexus</id>
          <repositories>
            <repository>
              <id>maven-host</id>
              <url>http://nexus-service.devops-tools.svc.cluster.local:8081/repository/maven-host/</url>
              <releases>
                <enabled>true</enabled>
